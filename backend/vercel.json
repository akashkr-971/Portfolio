{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "vite.config.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api/server.js"
        }
    ]
}